// Deque is a double-ended queue that allows insertion, 
// deletion, and access from both the first and last ends.

import java.util.Deque;
import java.util.LinkedList;

public class ImplementDeque {

    public static void main (String args[]){
        Deque<Integer> deque = new LinkedList<>();
        deque.addFirst(1);
        deque.addFirst(2);
        deque.addLast(3);
        deque.addLast(4);
        deque.addFirst(5);
        deque.addLast(6);
        deque.addLast(7);
        System.out.println(deque);

        deque.removeFirst();
        deque.removeLast();

        System.out.println(deque.getLast());
        System.out.println(deque.getFirst());
        System.out.println(deque);
    }
}
